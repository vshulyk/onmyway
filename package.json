{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "NODE_ENV='development' node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "NODE_ENV='development' ./node_modules/.bin/mocha --compilers js:babel-register --require ./test/setup_dom.js --recursive --growl",
    "test:watch": "NODE_ENV='development' npm test -- --watch --growl",
    "test:cover": "NODE_ENV='development' ./node_modules/.bin/nyc --require babel-core/register ./node_modules/.bin/mocha --compilers js:babel-register --require ./test/setup_dom.js --recursive",
    "lint": "NODE_ENV='development' ./node_modules/.bin/eslint src",
    "lint:fix": "NODE_ENV='development' ./node_modules/.bin/eslint src --fix",
    "build": "NODE_ENV='production' ./node_modules/.bin/webpack -p"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-react": "^6.3.0",
    "expect": "^1.20.2",
    "growl": "^1.9.2",
    "istanbul": "^0.4.5",
    "jquery": "^2.2.1",
    "jsdom": "^8.5.0",
    "mocha": "^2.5.3",
    "nyc": "^8.3.0",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "geolib": "^2.0.21",
    "leaflet": "^0.7.7",
    "lodash": "^3.10.1",
    "morgan": "^1.7.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.30.5",
    "react-bootstrap-modal": "^3.0.0",
    "react-dom": "^0.14.3",
    "react-leaflet": "^0.12.2",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redis": "^2.6.2",
    "redux": "^3.0.4",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "uuid": "^2.0.2"
  }
}
